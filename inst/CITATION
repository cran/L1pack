citHeader("To cite the L1pack in publications use:")

citEntry(entry="Manual",
         title = "Routines for L1 estimation",
	 author = personList(as.person("Osorio, F.")),
	 year = "2015",
	 note = "R package version 0.3",
	 url = "http://www.ies.ucv.cl/l1pack/",
	 textVersion =
	 paste("Osorio, F. (2015). ",
               "Routines for L1 estimation. ",
	       "R package version 0.3. ",
	       "http://www.ies.ucv.cl/l1pack/", sep="")
)
